/**index.wxss**/
page {
  background-color: #f5f5f5;
}
.history_list{
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #fff;
  margin: 20rpx 0;
  padding: 12rpx 40rpx; 
  color: #333;
}
.create_time{
  padding-top: 8rpx;
  font-size: 26rpx;
  color:gray;
}

.arrow{
width: 10px;
height: 10px;
border-top: 2px solid #969696;
border-right: 2px solid #969696;
position: absolute;
right: 20rpx;
transform: rotate(45deg);
}
